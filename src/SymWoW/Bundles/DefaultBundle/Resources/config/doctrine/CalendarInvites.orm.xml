<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Symwow\Bundles\DefaultBundle\Entity\CalendarInvites" table="calendar_invites">
    <id name="id" type="bigint" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="event" type="bigint" column="event" nullable="false"/>
    <field name="invitee" type="integer" column="invitee" nullable="false"/>
    <field name="sender" type="integer" column="sender" nullable="false"/>
    <field name="status" type="boolean" column="status" nullable="false"/>
    <field name="statustime" type="integer" column="statustime" nullable="false"/>
    <field name="rank" type="boolean" column="rank" nullable="false"/>
    <field name="text" type="string" column="text" length="255" nullable="false"/>
  </entity>
</doctrine-mapping>
