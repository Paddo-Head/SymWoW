<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Symwow\Bundles\DefaultBundle\Entity\CalendarEvents" table="calendar_events">
    <id name="id" type="bigint" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="creator" type="integer" column="creator" nullable="false"/>
    <field name="title" type="string" column="title" length="255" nullable="false"/>
    <field name="description" type="string" column="description" length="255" nullable="false"/>
    <field name="type" type="boolean" column="type" nullable="false"/>
    <field name="dungeon" type="integer" column="dungeon" nullable="false"/>
    <field name="eventtime" type="integer" column="eventtime" nullable="false"/>
    <field name="flags" type="integer" column="flags" nullable="false"/>
    <field name="time2" type="integer" column="time2" nullable="false"/>
  </entity>
</doctrine-mapping>
