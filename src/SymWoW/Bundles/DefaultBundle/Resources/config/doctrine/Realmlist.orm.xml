<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Symwow\Bundles\DefaultBundle\Entity\Realmlist" table="realmlist">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="32" nullable="false"/>
    <field name="address" type="string" column="address" length="255" nullable="false"/>
    <field name="localaddress" type="string" column="localAddress" length="255" nullable="false"/>
    <field name="localsubnetmask" type="string" column="localSubnetMask" length="255" nullable="false"/>
    <field name="port" type="smallint" column="port" nullable="false"/>
    <field name="icon" type="boolean" column="icon" nullable="false"/>
    <field name="flag" type="boolean" column="flag" nullable="false"/>
    <field name="timezone" type="boolean" column="timezone" nullable="false"/>
    <field name="allowedsecuritylevel" type="boolean" column="allowedSecurityLevel" nullable="false"/>
    <field name="population" type="float" column="population" nullable="false"/>
    <field name="gamebuild" type="integer" column="gamebuild" nullable="false"/>
  </entity>
</doctrine-mapping>
