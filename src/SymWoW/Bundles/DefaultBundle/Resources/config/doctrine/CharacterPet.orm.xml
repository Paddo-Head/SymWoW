<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Symwow\Bundles\DefaultBundle\Entity\CharacterPet" table="character_pet">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="entry" type="integer" column="entry" nullable="false"/>
    <field name="owner" type="integer" column="owner" nullable="false"/>
    <field name="modelid" type="integer" column="modelid" nullable="true"/>
    <field name="createdbyspell" type="integer" column="CreatedBySpell" nullable="false"/>
    <field name="pettype" type="boolean" column="PetType" nullable="false"/>
    <field name="level" type="smallint" column="level" nullable="false"/>
    <field name="exp" type="integer" column="exp" nullable="false"/>
    <field name="reactstate" type="boolean" column="Reactstate" nullable="false"/>
    <field name="name" type="string" column="name" length="21" nullable="false"/>
    <field name="renamed" type="boolean" column="renamed" nullable="false"/>
    <field name="slot" type="boolean" column="slot" nullable="false"/>
    <field name="curhealth" type="integer" column="curhealth" nullable="false"/>
    <field name="curmana" type="integer" column="curmana" nullable="false"/>
    <field name="curhappiness" type="integer" column="curhappiness" nullable="false"/>
    <field name="savetime" type="integer" column="savetime" nullable="false"/>
    <field name="abdata" type="text" column="abdata" nullable="true"/>
  </entity>
</doctrine-mapping>
