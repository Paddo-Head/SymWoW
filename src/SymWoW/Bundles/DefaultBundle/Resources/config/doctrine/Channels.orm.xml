<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Symwow\Bundles\DefaultBundle\Entity\Channels" table="channels">
    <id name="name" type="string" column="name" length="128"/>
    <id name="team" type="integer" column="team"/>
    <field name="announce" type="boolean" column="announce" nullable="false"/>
    <field name="ownership" type="boolean" column="ownership" nullable="false"/>
    <field name="password" type="string" column="password" length="32" nullable="true"/>
    <field name="bannedlist" type="text" column="bannedList" nullable="true"/>
    <field name="lastused" type="integer" column="lastUsed" nullable="false"/>
  </entity>
</doctrine-mapping>
