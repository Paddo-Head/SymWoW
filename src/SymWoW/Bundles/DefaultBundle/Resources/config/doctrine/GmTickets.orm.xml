<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Symwow\Bundles\DefaultBundle\Entity\GmTickets" table="gm_tickets">
    <id name="ticketid" type="integer" column="ticketId">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="guid" type="integer" column="guid" nullable="false"/>
    <field name="name" type="string" column="name" length="12" nullable="false"/>
    <field name="message" type="text" column="message" nullable="false"/>
    <field name="createtime" type="integer" column="createTime" nullable="false"/>
    <field name="mapid" type="smallint" column="mapId" nullable="false"/>
    <field name="posx" type="float" column="posX" nullable="false"/>
    <field name="posy" type="float" column="posY" nullable="false"/>
    <field name="posz" type="float" column="posZ" nullable="false"/>
    <field name="lastmodifiedtime" type="integer" column="lastModifiedTime" nullable="false"/>
    <field name="closedby" type="integer" column="closedBy" nullable="false"/>
    <field name="assignedto" type="integer" column="assignedTo" nullable="false"/>
    <field name="comment" type="text" column="comment" nullable="false"/>
    <field name="response" type="text" column="response" nullable="false"/>
    <field name="completed" type="boolean" column="completed" nullable="false"/>
    <field name="escalated" type="boolean" column="escalated" nullable="false"/>
    <field name="viewed" type="boolean" column="viewed" nullable="false"/>
    <field name="haveticket" type="boolean" column="haveTicket" nullable="false"/>
  </entity>
</doctrine-mapping>
