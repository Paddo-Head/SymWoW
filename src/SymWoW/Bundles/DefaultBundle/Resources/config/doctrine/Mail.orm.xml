<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Symwow\Bundles\DefaultBundle\Entity\Mail" table="mail">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="messagetype" type="boolean" column="messageType" nullable="false"/>
    <field name="stationery" type="boolean" column="stationery" nullable="false"/>
    <field name="mailtemplateid" type="smallint" column="mailTemplateId" nullable="false"/>
    <field name="sender" type="integer" column="sender" nullable="false"/>
    <field name="receiver" type="integer" column="receiver" nullable="false"/>
    <field name="subject" type="text" column="subject" nullable="true"/>
    <field name="body" type="text" column="body" nullable="true"/>
    <field name="hasItems" type="boolean" column="has_items" nullable="false"/>
    <field name="expireTime" type="integer" column="expire_time" nullable="false"/>
    <field name="deliverTime" type="integer" column="deliver_time" nullable="false"/>
    <field name="money" type="integer" column="money" nullable="false"/>
    <field name="cod" type="integer" column="cod" nullable="false"/>
    <field name="checked" type="boolean" column="checked" nullable="false"/>
  </entity>
</doctrine-mapping>
